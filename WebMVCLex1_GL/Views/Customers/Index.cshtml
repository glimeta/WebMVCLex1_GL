@{
}

@model List<Customer>

<h1>Customer Index</h1>

<table>
    <tr> 
        <td>First Name</td>
        <td>Last Name</td>
        <td>SSN</td>

    </tr>

    @foreach(Customer c in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => c.FirstName)
            </td>

            <td>
                @Html.DisplayFor(modelItem => c.LastName)
            </td>

            <td>
                @Html.DisplayFor(modelItem => c.SSN)
            </td>
        </tr>
    }


</table>
